  <form:scripts>

page_afterDataSourceLoad:function(){
	$view.slaproductrel.setParams({&quot;serviceProductNo&quot;:$params.serviceproduct.productNo},true);
	$view.ucproductrellist.setParams({&quot;serviceProductNo&quot;:$params.serviceproduct.productNo},true);	
},
//sla 指标项
groupId_customFormatter:function(cellvalue,options,rowObject){
//	console.log(cellvalue);
	var result = &quot;&lt;table border='0'>&quot;;
	for(var p in cellvalue){
		result = result+&quot;&lt;tr>&quot;+&quot;&lt;td>&quot;+p+&quot;&lt;/td>&quot;+&quot;&lt;td>&quot;+cellvalue[p]+&quot;&lt;/td>&quot;+&quot;&lt;/tr>&quot;;
	}
	result = result+&quot;&lt;/table>&quot;
	return result;
},
//uc 展示服务时间
relTypeId_customFormatter:function(cellvalue,options,rowObject){
	return cellvalue.serviceTimeDayOfWeek+&quot;天*&quot;+cellvalue.serviceTimeTimeOfDay+&quot;小时&quot;;
}</form:scripts>

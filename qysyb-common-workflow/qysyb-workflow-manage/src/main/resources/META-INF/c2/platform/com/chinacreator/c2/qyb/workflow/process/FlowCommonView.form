newField2_click:function($event){
	var paramsjson = {
		processInstanceId:$params.params.processInstanceId,
		processDefinitionId:$params.params.processDefinitionId
	}
	Modal.open(&quot;f/monitorProcess&quot;,paramsjson);			
},


page_onload:function(){
	$scope.operatefuncs = {}
	$scope.operatefuncs.processmonitor = functions.newField2_click
},

page_afterDataSourceLoad:function(){

//	$model.actions = $model.activityactions.result;
	$model.selectfuncs = new Array();
	//tab页选择事件
	$scope.$on(&quot;tabselect&quot;,function(event,msg){
//		console.log(&quot;recmsg&quot;,msg);
		$model.selectfuncs.push(msg);
	});
//	console.log(&quot;params&quot;,$params);


	//字段页面验证函数
	$scope.$on('func',function(event,msg){
		$model.func = msg;
	});
	//字段页面提交流程前执行函数
	$scope.$on('funcsbeforsubmit',function(event,msg){
		$model.funcsbeforsubmit = msg;
	});
	
	$model.needqueryprocdata = !angular.isUndefined($params.params.processInstanceId) 
		&amp;&amp; (angular.isUndefined($params.params.businessKey) || angular.isUndefined($params.params.moduleId))
	if($model.needqueryprocdata){
		$http.post(&quot;ws/getprocinfobyprocinsid&quot;,{&quot;procInsId&quot;:$params.params.processInstanceId}).success(function(data){
			var procInfo = data.result
			$params.params.businessKey = procInfo.businessKey;
			$params.params.processDefinitionId =  procInfo.processDefinitionId; 
			$params.params.moduleId = procInfo.moduleId;
			functions.getviewinfo($params.params.moduleId,$params.params.processInstanceId,$params.params.businessKey)
			return
		})
	}else{
		functions.getviewinfo($params.params.moduleId,$params.params.processInstanceId,$params.params.businessKey)
	}


},
getviewinfo:function(moduleId,procInsId,businessKey){
	var qs = [
	          $http.post(&quot;ws/getformfiledpermissiondataforview1&quot;,{&quot;moduleId&quot;:moduleId}), 		
	          $http.post(&quot;ws/getalltabsforview1&quot;,{&quot;moduleId&quot;:moduleId,params:$params.params}),
	          $http.post(&quot;ws/getserviceproductbyid&quot;,{&quot;id&quot;:moduleId}), 	
	          ]


    $q.all(qs).then(function(resps){
    	$scope.permissionData = resps[0].data.result;
    	$model.dynamictabs = resps[1].data.result;
    	$model.serviceproduct = resps[2].data
    	
		if(&quot;undefined&quot; != typeof WORKFLOW_CONS){
			var personality = WORKFLOW_CONS.PERSONALITY;
			if(personality!=undefined&amp;&amp;!angular.isUndefined(personality[$model.serviceproduct.result.productNo])){
				$model.baseinfo = personality[$model.serviceproduct.result.productNo].BASEINFO;
			}	
		}else{
				$model.baseinfo = &quot;基本信息&quot;;
		}
    	$model.formId = $model.serviceproduct.result.formId;
    	
		$model.formBusinessKey = $params.params.businessKey;
		$params.params.formBusinessKey = $model.formBusinessKey;
	
		$params.params.serviceproduct = $model.serviceproduct.result;
		$params.params.servicetypes = $model.servicetypes.result;
		var locationparams = $location.$$search;    
		$http.post(&quot;ws/queryformdatafromprovariable&quot;,{&quot;formId&quot;:$model.serviceproduct.result.formId,&quot;filter&quot;:true,&quot;businessKey&quot;:$model.formBusinessKey,&quot;proInsId&quot;:$params.params.processInstanceId}).success(function(data){	
			$model.formentity = {};
			$model.formentity.result = data.result;
			$scope.formentity = $model.formentity.result;
			//全局 用来通信
			$scope.formentity.global = {}			
	//		console.log($model.formentity.result);
			//取得地址栏中的参数 赋值地址栏参数到实体
			for(var a in locationparams){
				$model.formentity.result[a] = locationparams[a];
			}	

 			if($model.dynamictabs.length == 0){
 				$model.tabDisplay = false
 			}else{
 				$model.tabDisplay = true
 			}	
			if($model.tabDisplay){
				$model.tabUrl = 'f/wfbusinessformgenerate'
			}else{
				$model.singleUrl = 'f/wfbusinessformgenerate'
			}				
		});			
    })
},
dateconvert:function(a){
	var date = new Date(a);
	return date.Format(&quot;MM/dd hh:mm&quot;);;
},

page_beforeDataSourceLoad:function($params){
//	console.log($params);
	console.log(&quot;params&quot;,$params);
	var locationparams = $location.$$search;
	console.log(&quot;locationparams&quot;,locationparams);
	$params.params = {};
	$params.params.moduleId = locationparams.moduleId || $params.moduleId;
	$params.params.activityId = locationparams.activityId || $params.activityId;
	$params.params.taskDefKey = locationparams.activityId || $params.activityId;
	$params.params.businessKey = locationparams.businessKey || $params.businessKey;
	$params.params.processInstanceId = locationparams.proInsId || $params.proInsId;
	$params.params.processDefinitionId =  locationparams.processDefinitionId || $params.processDefinitionId;
	//不止为何 数据源中的参数不能再￥params.params中取
	$params.moduleId = locationparams.moduleId;
},

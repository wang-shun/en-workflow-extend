	$http.post(&quot;ws/updateFormDataWithExternalTable&quot;,{&quot;businessKey&quot;:businessKey,&quot;entityjson&quot;:$model.entity,&quot;form&quot;:form}).success(function(){
		TabOperator.closeTab(TabOperator.getCurrentTab());
	});

  <form:scripts>
//选择路径事件
newField_change:function(){
	$params.nextname = $view.nexttaskview.getOptionObj($model.nexttask).dest.name;
	if($params.nextname==undefined){
		$model.transition = $view.nexttaskview.getOptionObj($model.nexttask);
	//	console.log(&quot;transtion&quot;,$model.transition);
		$model.dest = $model.transition.dest;
		//如果最后一步是结束事件
		if($model.dest.porperties.type==&quot;endEvent&quot;){ 
			$params.nextname = &quot;结束&quot;;
		}	
		//如果下一步是会签事件
		if($model.dest.porperties.multiInstance==&quot;parallel&quot;){
			$model.isParallel = true;
		}
	}
	
	if($params.nextname == &quot;反馈申请人&quot;){
		$model.nexthandleid = $model.entity.applicantId;
		$model.isAssigneeApplicant = true;
	}else{
		$model.nexthandleid = null;
		$model.isAssigneeApplicant = false;
	}


	$http.get(&quot;ws/getactivitycandidates&quot;,{
		params:{
			processDefinitionId:$params.params.processDefinitionId,
			moduleId:$params.params.moduleId,
			taskDefKey:$view.nexttaskview.getOptionObj($model.nexttask).dest.id,
		}}).success(function(data){
			$model.nextgroups = data.result;
			//默认选中第一个处理组吧
			$model.nexthandlegroupid = $model.nextgroups[0].id;
			$http.get(&quot;ws/getuserjobbyid&quot;,{
					params:{
						jobid:$model.nexthandlegroupid,
					}}).success(function(data){
						$model.handles = data.result;
					//	$model.handleid = $model.handles[0].userId;
					});	

		});	
},
//选择处理人
newField3_customEven:function(){
	Modal.open(&quot;f/chooespersonfromgroup&quot;,{groups:$model.nextgroups},
			function(data){
				console.log(data);
				var candidateUsersStr = &quot;&quot;;
				for(var i=0;i&lt;data.selected.length;i++){
					candidateUsersStr=candidateUsersStr+data.selected[i].id+&quot;,&quot;;
				}
				$model.candidateUsersStr = candidateUsersStr;
				},
			function(data){
				
			});	
},
//提交
commit:function($event){

	//func 为业务表单提交前验证函数
	if(!angular.isUndefined($model.func)){
		var a = $model.func();
		if(a==false){
			Messenger.post({
				'message': &quot;必填项为空！&quot;,
	            'type': 'error',
	        });	
			return;
		}	
	}
//	console.log(&quot;$model.funcsbeforsubmit&quot;,$model.funcsbeforsubmit);
	//必填项等验证通过才执行 字段页面提交前函数
	$model.funcsbeforsubmit();
/*	if(angular.isUndefined($model.opinion)){
		Messenger.post({
		    'message': &quot;请填写处理意见！&quot;,
		    'type': 'error',
		});		
		return;
	}*/
	
	var destroad = $view.nexttaskview.getOptionObj($model.nexttask);
	var dest = $view.nexttaskview.getOptionObj($model.nexttask).dest;
//	console.log(&quot;dest&quot;,dest);
	if(dest.porperties.type==&quot;endEvent&quot;){ //下一步是结束节点 不选人
		functions.newField5_click($event);
		return;
	}
	if($model.isCountersign){  //会签环节 不选人，下一步处理人用环节外围配置
		functions.newField5_click($event);
		return;		
	}
	if($model.actions.freechoose!=undefined){
		var url = $model.actions.freechoose.openUrl;
		Modal.open(url,{params:$params.params,transitions:$model.getOutTransition.result},function(data){
			console.log(&quot;choosed&quot;,data);
			$model.assigneeList = data.assigneeList==&quot;&quot;?undefined:data.assigneeList;//避免传空字符进去，影响判断;
			$model.assigneeid = data.assignee==&quot;&quot;?undefined:data.assignee;
			$model.nexthandlegroupid = data.candidateGroups==&quot;&quot;?undefined:data.candidateGroups;
			$model.nexthandleid = data.candidateUsers==&quot;&quot;?undefined:data.candidateUsers;
			
			$model.transition = data.transition;
			$model.dest = $model.transition.dest;
			$model.nexttask = $model.transition.id;
			functions.newField5_click($event);
			
		});
		return;
	}
	functions.newField5_click($event);
},
//提交
newField5_click:function($event){
	//1117test
	var transitionId = $model.nexttask;
//	console.log(&quot;transitionId&quot;,transitionId);

	$model.formId = $model.serviceproduct.result.formId;
//	console.log(&quot;entitybeforsubmit&quot;,$model.entity);
	if($model.isAssigneeApplicant){
		$model.nexthandleid = $model.entity.applicantId;
	}
	if(!angular.isUndefined($scope.func)){
		var a = $scope.func();	
	}	
	
	var userId =  $model.getUser.result.id;
	var processDefinitionId = $params.params.processDefinitionId;
	var moduleId =  $params.params.moduleId;
	var taskDefKey =  $params.params.taskDefKey;
	variables = {
		assignee:$model.assigneeid,
		candidateGroups:$model.nexthandlegroupid,
		candidateUsers:$model.nexthandleid,
		assigneeList:$model.assigneeList,
		wfComment:$model.opinion
	}
	if($model.handleway==&quot;处理完成&quot;){
		$http.post(&quot;flow/goanywhere&quot;,
			{
				transition:$model.transition,
				transitionId:transitionId,
				formId:$model.formId,
				wfOperator:{
					userId:userId,
					businessData:{
						moduleId:moduleId,
						businessKey:$params.businessKey
					}
			    },
			    businessKey:$params.params.businessKey,
				isStart:false,
				processDefinitionId:$params.params.processDefinitionId,
				moduleId:$params.params.moduleId,
				currenTaskId:$params.params.taskId,
				destTaskDefinitionKey:$model.dest.id,
				useHisAssignee:false,
				variables:variables,
				entity:$model.entity,
				proInsId:$params.params.processInstanceId,
				taskDefKey:$params.params.taskDefKey,
				opinion:$model.opinion
			
		}).success(function(data){
			if(data.result == &quot;200&quot;){
				 Messenger.post({
					 'message': &quot;  任务处理完成！&quot;,
					 'type': 'success',
					 'hideAfter':5
				 });
				 TabOperator.closeTab(TabOperator.getCurrentTab());
			}else{
				 Messenger.post({
					 'message': &quot;  something wrong！&quot;,
					 'type': 'error',
					 'hideAfter':5
				 });			
			}
		});	
	}else if($model.handleway==&quot;退回&quot;){
		$http.get(&quot;flow/reject&quot;,
		{params:{
			clazz:$model.clazz,
			wfOperator:{
				userId:userId,
				businessData:{
					moduleId:moduleId,
					businessKey:$params.params.businessKey
				}
		    },
		    businessKey:$params.params.businessKey,
			currenTaskId:$params.params.taskId,
			processDefinitionId:$params.params.processDefinitionId,
			taskDefKey:$params.params.taskDefKey,
			variables:variables,
			entity:$model.entity,
			rejectMessage:&quot;reject&quot;,
		}
	}).success(function(data){
			console.log(data);
			 Messenger.post({
			    'message': data.result.result,
			    'type': 'error',
			});
		});		
	}

	
},

newField2_click:function($event){
	var paramsjson = {
		processInstanceId:$params.params.processInstanceId,
		processDefinitionId:$params.params.processDefinitionId
	}
	Modal.open(&quot;f/monitorProcess&quot;,paramsjson);			
},



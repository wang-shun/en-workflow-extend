},
//回退
newField_click:function($event){
	var userId =  $model.getUser.result.id;
	var processDefinitionId = $params.params.processDefinitionId;
	var moduleId =  $params.params.moduleId;
	var taskDefKey =  $params.params.taskDefKey;
	variables = {
//		assignee:&quot;68C01B1AA07E4390A7CF8997E1DF83DC&quot;,
//		candidateGroups:$model.candidateGroup,
//		candidateUsers:$model.candidateUsersStr,
//		wfComment:$model.opinion
	}
	$http.post(&quot;flow/reject&quot;,
		{
			rejectMessage:&quot;reject&quot;,
			
			formId:$model.formId,
			wfOperator:{
				userId:userId,
				businessData:{
					moduleId:moduleId,
					businessKey:$model.businessKey
				}
		    },
		    businessKey:$params.params.businessKey,
			isStart:false,
			processDefinitionId:$params.params.processDefinitionId,
			moduleId:$params.params.moduleId,
			currenTaskId:$params.params.taskId,
			destTaskDefinitionKey:$model.actions.return.returnTo,
			useHisAssignee:true,
			variables:variables,
			entity:$model.entity,
			proInsId:$params.params.processInstanceId,
			taskDefKey:$params.params.taskDefKey,
			opinion:$model.opinion		
	}).success(function(data){
			 Messenger.post({
			    'message': &quot;退回成功&quot;,
			    'type': 'success',
			    'hideAfter':5
			 });
			 TabOperator.closeTab(TabOperator.getCurrentTab());
		});		
},

newField2_clickEvent:function(value,checked,checkboxObj){
	var radioGroup = $view.radios.getOptions();  
	if(value.value==&quot;处理完成&quot;){
		$view.nextdiv.show();
		$view.chulirendiv.show();
	}
	if(value.value==&quot;退回&quot;){
		$view.nextdiv.hide();
		$view.chulirendiv.hide();
	}
	angular.forEach(radioGroup,function(v){  
		//如果选项值为2，添加该选项的点击事件  
		if(v.value!=value.value){  
			v.checked = false;  
		}  
	}); 
},

page_afterDataSourceLoad:function(){
//	console.log(&quot;sp&quot;,$model.serviceproduct.result);
	if(&quot;undefined&quot; != typeof WORKFLOW_CONS){
		var personality = WORKFLOW_CONS.PERSONALITY;
		if(personality!=undefined&amp;&amp;!angular.isUndefined(personality[$model.serviceproduct.result.productNo])){
			$model.baseinfo = personality[$model.serviceproduct.result.productNo].BASEINFO;
		}	
	}else{
			$model.baseinfo = &quot;基本信息&quot;;
	}

	$model.actions = $model.activityactions.result;
	$model.selectfuncs = new Array();
	//tab页选择事件
	$scope.$on(&quot;tabselect&quot;,function(event,msg){
//		console.log(&quot;recmsg&quot;,msg);
		$model.selectfuncs.push(msg);
	});
//	console.log(&quot;params&quot;,$params);
	$model.isAssigneeApplicant = false;
	$scope.$on('entity',function(event,msg){
		$model.entity = msg.entity;
		$scope.entity = msg.entity;
	});	
	//字段页面验证函数
	$scope.$on('func',function(event,msg){
		$model.func = msg;
	});
	//字段页面提交流程前执行函数
	$scope.$on('funcsbeforsubmit',function(event,msg){
		$model.funcsbeforsubmit = msg;
	});
	$scope.$on('applicant',function(event,applicant){
		$scope.applicant = applicant;
	});
	$scope.$on('applicantName',function(event,applicantName){
		$scope.applicantName = applicantName;
	});
	$model.activityName = $model.getOutTransition.result[0].src.name;

	$model.formId = $model.serviceproduct.result.formId;
	$http.post(&quot;ws/getformfiledpermissiondata&quot;,{&quot;formId&quot;:$model.formId,&quot;businessKey&quot;:$params.params.taskDefKey}).success(function(data){
//		$scope.$apply(function(){$scope.permissionData = data.result;});	
		$scope.permissionData = data.result;
	});
//  businessKey processInstanceId不为null，表明是处理任务而不是提交
//  formBusinessKey 一定有业务数据与其对应 $model.businessKey则有可能是新生成的Key
	if($params.params.businessKey&amp;&amp;$params.params.processInstanceId!=null){
		$model.businessKey =  $params.params.businessKey;	
		$model.formBusinessKey = $model.businessKey;
	//如果地址栏有businessKey，则是业务数据已保存过，或许是草稿 或许是因为快捷生成工单。需要拿出之前的业务数据
	}else if(angular.isUndefined($params.params.businessKey)&amp;&amp;!angular.isUndefined($location.$$search.businessKey)){
		$model.businessKey =  $location.$$search.businessKey;
		$model.formBusinessKey = $model.businessKey;
	}else{
		$model.businessKey =  $params.params.moduleId+&quot;_&quot;+this.timeGenerator();
	}
	$params.params.formBusinessKey = $model.formBusinessKey;
/*	if(angular.isUndefined($params.params.businessKey)){
		$params.params.businessKey = $model.businessKey;
	}*/
	var paramsfortabs = $params.params;
	paramsfortabs.businessKey = $model.businessKey;
	paramsfortabs.serviceproduct = $model.serviceproduct.result;
	paramsfortabs.servicetypes = $model.servicetypes.result;
	$http.post(&quot;ws/getalltabsforactivity&quot;,{productId:$params.params.moduleId,params:$params.params,activityId:$params.params.taskDefKey}).success(function(data){
//		console.log(&quot;tabs&quot;,data);
		$model.dynamictabs = data.result;
	});
	//取得地址栏中的参数
	var locationparams = $location.$$search;

	$http.post(&quot;ws/queryformdatafromprovariable&quot;,{&quot;formId&quot;:$model.serviceproduct.result.formId,&quot;filter&quot;:true,&quot;businessKey&quot;:$model.formBusinessKey,&quot;proInsId&quot;:$params.params.processInstanceId}).success(function(data){	
		$model.formentity = {};
		$model.formentity.result = data.result;
//		console.log($model.formentity.result);
		//取得地址栏中的参数 赋值地址栏参数到实体
		for(var a in locationparams){
			$model.formentity.result[a] = locationparams[a];
		}		
		console.log(&quot;formentity&quot;,$model.formentity.result);
		//工单编号的自动赋值 ugly TODO
		if(angular.isUndefined($model.formentity.result[WORKNOKEY])||$model.formentity.result[WORKNOKEY]==&quot;&quot;){
			$http.post(&quot;ws/numberwithtimestamp&quot;,{prefix:$model.serviceproduct.result.productNo+&quot;_&quot;})
				.success(function(a){
					$model.formentity.result[WORKNOKEY] = a.result;
					$model.in = $params.businessForm;
				});
		}else{
			$model.in = $params.businessForm;
		}
	});


	
	$model.handleway = &quot;处理完成&quot;;
	
//默认选中第一个步骤并触发值变换事件
	$model.nexttask = $model.getOutTransition.result[0].id;	

//当前环节属性
	var ac = $model.getOutTransition.result[0].src.porperties;

	if(!angular.isUndefined(ac)&amp;&amp;ac.multiInstance == &quot;parallel&quot;){ //并行会签
		var item = $model.getOutTransition.result[0];
		item.name = &quot;多人处理&quot;;
		$model.isCountersign = true;
		$model.getOutTransition.result = new Array();
		$model.getOutTransition.result[0] = item;
	}
	$model.transition = $model.getOutTransition.result[0];

	$model.dest = $model.transition.dest;
	//如果下一步是最后一步结束事件
	if($model.dest.porperties.type==&quot;endEvent&quot;){ 
		$params.nextname = &quot;结束&quot;;
	}
	//如果下一步是会签事件
	if($model.dest.porperties.multiInstance==&quot;parallel&quot;){
		$model.isParallel = true;
	}


	$http.get(&quot;ws/getactivitycandidates&quot;,{
		params:{
			processDefinitionId:$params.params.processDefinitionId,
			moduleId:$params.params.moduleId,
			taskDefKey:$model.getOutTransition.result[0].dest.id,
		}}).success(function(data){
			$model.nextgroups = data.result;
			//默认选中第一个处理组吧
//			console.log(&quot;11111111&quot;,data);
			$model.nexthandlegroupid = $model.nextgroups[0].id;
			$http.get(&quot;ws/getuserjobbyid&quot;,{
					params:{
						jobid:$model.nexthandlegroupid,
					}}).success(function(data){
						$model.handles = data.result;
					//	$model.handleid = $model.handles[0].userId;
					});	
			$params.nextname = $view.nexttaskview.getOptionObj($model.nexttask).dest.name;
			if($params.nextname == &quot;反馈申请人&quot;){
				$model.isAssigneeApplicant = true;
			}
		});	
},

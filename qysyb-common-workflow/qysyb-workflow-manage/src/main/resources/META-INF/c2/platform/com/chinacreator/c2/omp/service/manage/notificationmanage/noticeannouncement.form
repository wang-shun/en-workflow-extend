  <form:scripts>

query_click:function($event){
	$view.list.refresh($params.list);
},&#xD;
&#xD;


reset_click:function($event){
		//清空所有表单元素的值
	$view.mainForm.clean();		
},
buttonEdit_click:function(rowId,rowData,tableData){
	var selectNodes = $view.list.getSelectDatas();
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){
		if(selectNodes.length>1){
			Messenger.post({
			    'message': &quot;一次只能编辑一条信息！&quot;,
			    'type': 'error',
			});
			return;
		}else{
			var selectNode = selectNodes[0]; 
			 Modal.open('ef/daily_notice/edit',{'notifyId':selectNode.notifyId},function(result){
				$view.list.refresh();
			});	
		}	
	}else{
		Messenger.post({
		    'message': &quot;请选择需要编辑的信息！&quot;,
		    'type': 'error',
		});
	}		
},


//页面显示发布状态
state_customFormatter:function(cellvalue,options,rowObject){
	return cellvalue?'&lt;span class=&quot;1&quot;>已发布&lt;/span>':'&lt;span class=&quot;0&quot;>未发布&lt;/span>';
},

page_onload:function(){
	
	
},





newField_click:function(rowId,rowData,tableData){
	var selectNodes = $view.list.getSelectDatas();
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){
		if(selectNodes.length>1){
			Messenger.post({
			    'message': &quot;一次只能查看一条信息！&quot;,
			    'type': 'error',
			});
			return;
		}else{
			var selectNode = selectNodes[0]; 
			 Modal.open('ef/daily_notice/check',{notifyXh:selectNode.notifyId},function(result){
				$view.list.refresh();
			});	
		}	
	}else{
		Messenger.post({
		    'message': &quot;请选择需要查看的信息！&quot;,
		    'type': 'error',
		});
	}		
},

buttonAdd_click:function(rowId,rowData,tableData){
	/*注意注意这里有个bug！每次add时必须重新load getUUID 目前没时间改 谁改下吧 0121*/
	var notifyXh = $model.getUUID.result;
	Modal.open(&quot;ef/daily_notice/add&quot;,{notifyXh:notifyXh},function(result){
	    $view.list.refresh();
	});
},

newField_customFormatter:function(cellvalue,options,rowObject){
	var now = new Date().getTime();
	//alert(rowObject.endDate);
	if(now>rowObject.endDate){return '失效';}else{return '有效';}
}</form:scripts>

  <form:scripts>

colJqgrid_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return '&lt;a href=&quot;javascript:void(0)&quot; ng-click=functions.titleclick(&quot;'+rowObject.PROC_INST_ID+'&quot;)>'+cellvalue+'&lt;/a>'	
},
titleclick:function(proInsId){
	location.href = &quot;#/f/flowcommonview?proInsId=&quot;+proInsId
},

page_onload:function(){

},
//query
button_click:function($event){
	$view.list.refresh($params.search);
},
//reset
button1_click:function($event){
	$params.search.title = undefined
	$params.search.sendModuleName = undefined
	$view.list.refresh($params.search);
},
//查看
buttonView_click:function(rowId,rowData,tableData){
	var selected = $view.list.getSelectDatas()
	if(selected &amp;&amp; selected.length == 1){
		var instId = selected[0].PROC_INST_ID
		functions.titleclick(instId)
	}else{
		Messenger.error(&quot;请选择或只选择一条记录&quot;)
	}
},

buttonATable_click:function(rowId,rowData,tableData){
	var selected = $view.list.getSelectDatas()
	if(selected &amp;&amp; selected.length == 1){
		Modal.openConfirm({message:&quot;标记选择为已阅？&quot;},function(){
			$http.post(&quot;ws/setprocreaded&quot;,{id:rowId}).success(function(){
				$view.list.refresh($params.search);
			})
		})
	}else{
		Messenger.error(&quot;请选择或只选择一条记录&quot;)
	}
}</form:scripts>

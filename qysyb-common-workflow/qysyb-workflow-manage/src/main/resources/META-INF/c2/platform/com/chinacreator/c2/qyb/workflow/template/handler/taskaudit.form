        &lt;/button>
        &lt;button c2-button=&quot;&quot; id=&quot;button3&quot; type=&quot;button&quot; class=&quot;c2-button btn  btn-primary btn-sm no-border&quot; 
            ng-init=&quot;$state.button3.disabled = false;$state.button3.visible=!$model.isStart&quot;
            ng-show=&quot;!$model.isStart&quot; ng-disabled=&quot;$state.button3.disabled&quot; show-default=&quot;false&quot; disabled-default=&quot;false&quot;
            ng-click=&quot;functions.button3_click($event)&quot;>

    <form:child xsi:type="form:Group" id="newGroup7" name="简单容器(DIV)">
      <form:control type="container/div">
        <form:attributes source="container/div">
          <dync:attribute default="false" group="HTML配置" id="nopadding" name="无内边距" value="true">
            <dync:render/>
            <dync:children/>
          </dync:attribute>
        </form:attributes>
      </form:control>
      <form:child xsi:type="form:Group" binding="$model.list" id="listasset" name="列表型数据源">
        <form:control type="container/table_jqgrid">
          <form:attributes source="container/table_jqgrid">
            <dync:attribute default="" group="基本配置" id="caption" name="表头" value="故障设备">
              <dync:render/>
              <dync:children/>
            </dync:attribute>
            <dync:attribute default="" group="表格设置" id="paramData" name="初始化参数" value="{&quot;businessKey&quot;:$params.businessKey}">
              <dync:render/>
              <dync:children/>
            </dync:attribute>
          </form:attributes>
        </form:control>
        <form:child xsi:type="form:Group" id="toolbar_table1" name="toolbar_table">
          <form:control type="container/toolbar_table">
            <form:attributes source="container/toolbar_table"/>
          </form:control>
          <form:child xsi:type="form:Field" id="buttonAdd1" name="关联故障设备" res="yUJsd4bgTwGilNu4qJWoxQ">
            <form:control type="widget/button_a_table">
              <form:attributes source="widget/button_a_table">
                <dync:attribute id="icon" value="fa-plus">
                  <dync:render/>
                  <dync:children/>
                </dync:attribute>
                <dync:attribute id="iconColor" value="purple">
                  <dync:render/>
                  <dync:children/>
                </dync:attribute>
                <dync:attribute default="" group="事件设置" id="click" name="单击事件" value="buttonAdd_click(rowId,rowData,tableData)">
                  <dync:render/>
                  <dync:children/>
                </dync:attribute>
              </form:attributes>
            </form:control>
          </form:child>
          <form:child xsi:type="form:Field" id="newField7" name="刷新" res="YwJATq-1TOOjyvKDBKFikQ" customize="true">
            <form:control type="widget/button_a_table">
              <form:attributes source="widget/button_a_table">
                <dync:attribute default="custom" group="事件设置" id="clickType" name="事件类型" value="refrash">
                  <dync:render/>
                  <dync:children/>
                </dync:attribute>
                <dync:attribute default="" group="HTML设置" id="icon" name="图标" value="fa-refresh">

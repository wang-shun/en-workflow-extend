        <form:child xsi:type="form:Field" binding="name" id="assetWorkRelId1" name="资源名称">
          <form:control type="widget/col_jqgrid">
            <form:attributes source="widget/col_jqgrid"/>
          </form:control>
        </form:child>
        <form:child xsi:type="form:Field" binding="categoryId" id="assetId1" name="资源分类">

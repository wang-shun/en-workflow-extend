  <form:datasources>
    <form:datasource xsi:type="form:Inputs"/>
    <form:datasource xsi:type="form:PagingDataSource" id="list" name="列表型数据源" logic="com.chinacreator.c2.omp.service.manage.workflowcommon.impl.WorkRelAssetArrayContentProvider" pageSize="10" type="custom"/>
  </form:datasources>
  <form:scripts>//add relate
buttonAdd_click:function(rowId,rowData,tableData){
	Modal.open('f/assetChooseForm',function(){},function(a){
		var b = new Array();
		b.push(a.id);
		$http.post(&quot;ws/addWorkAssetRel&quot;,{'businessKey':$model.businessKey,'ids':b}).success(function(){
			$view.listasset.setParams({&quot;businessKey&quot;:$model.businessKey});
		});
	});
},
// del relate
newField6_click:function(rowId,rowData,tableData){
	var ids = $view.listasset.getSelectRowIds();
	$http.post(&quot;ws/delWorkAssetRel&quot;,{'ids':ids,'businessKey':$model.businessKey}).success(function(){
				Messenger.post({
					 'message': &quot;删除成功&quot;,
					 'type': 'success',
				 });
				 $view.listasset.setParams({&quot;businessKey&quot;:$model.businessKey});
	})
}</form:scripts>
</form:Form>

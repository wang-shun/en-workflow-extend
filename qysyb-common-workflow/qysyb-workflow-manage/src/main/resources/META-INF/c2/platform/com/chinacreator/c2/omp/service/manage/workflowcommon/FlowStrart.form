  <form:scripts>timeGenerator:function(){
	 var day=new Date();
	   var Year=0;
	   var Month=0;
	   var Day=0;
	   var Hour = 0;
	   var Minute = 0;
	   var Second = 0;
	   var CurrentDate=&quot;&quot;;
	   //初始化时间
	   Year       = day.getYear();
	   Month      = day.getMonth()+1;
	   Day        = day.getDate()
	   Hour       = day.getHours();
	   Minute     = day.getMinutes();
	   Second     = day.getSeconds();
	   CurrentDate = (Year+&quot;&quot;)+(&quot;&quot;+Month)+(Day+&quot;&quot;)+(&quot;&quot;+Hour)+(&quot;&quot;+Minute)+(Second+&quot;&quot;);
	   return CurrentDate;
		
},
//启动工单
newField5_click:function($event){
	if(!angular.isUndefined($model.func)){
		var a = $model.func();
		if(a==false){
			Messenger.post({
				'message': &quot;必填项为空！&quot;,
	            'type': 'error',
	        });	
			return;
		}	
	}

	if($model.isCommit){
		Messenger.post({
			'message': &quot;工单已提交，请勿重复提交！&quot;,
            'type': 'error',
        });	
        return;
	}
	var processDefinitionId = $params.params.processDefinitionId;
	var moduleId =  $params.params.moduleId;
	var taskDefKey =  $params.params.taskDefKey;
	var userId = $model.getUser.result.id;
	var applyBranchId = $model.entity.applyBranchId;
	variablesJson = {&quot;productNo&quot;:$location.search().productNo,&quot;applyBranchId&quot;:applyBranchId};
//	if(angular.isUndefined($model.entity.vmmachineId)){
//		Messenger.post({
//			'message': &quot;请点击编辑云主机，配置云主机信息&quot;,
//            'type': 'error',
//        });		
//       return;	
//	}
	$model.entity.businessKey = $model.businessKey;
	$model.entity.moduleId = moduleId;
	$http.get(&quot;flow/startflow&quot;,
			{
					params:
					{	
						clazz:$model.clazz,
						businessKey:$model.businessKey,
						processDefinitionId:processDefinitionId,
						variables:
							variablesJson,
						entity:$model.entity,
						wfOperator:{
							userId:userId,
							businessData:{
								moduleId:moduleId,
								businessKey:$model.businessKey
							}
					    }
					    
					}
			}
			).success(function(data){
					if(data.result=='200'){
							$model.isCommit = true;
							Messenger.post({
								'message': &quot;工单提交成功&quot;,
				                'type': 'success',
				            });
				
					}else{
							Messenger.post({
								'message': &quot;工单提交失败，请关闭本页面后重试&quot;,
				                'type': 'error',
				            });				
					}
		  });	
},

page_onload:function(){
	$model.entity = {};
	$scope.$on('entity',function(event,msg){
		$model.entity = msg.entity;
		$model.clazz = msg.clazz;
	});	
	$scope.$on('func',function(event,func){
		$model.func = func;
	});
	if($params.params.businessKey){
		$model.businessKey =  $params.params.businessKey;	
	}else{
		$model.businessKey =  $params.params.moduleId+&quot;_&quot;+this.timeGenerator();
	}
	$model.businessForm =  $params.businessForm;
	$model.entity.businessKey = $model.businessKey;
	$model.entity.moduleId = $params.params.moduleId;
	$model.includepage = $model.businessForm;

}</form:scripts>

newField1_click:function(rowId,rowData,tableData){
	//console.log(rowId);
	//console.log(rowData.taskId);
	//console.log(tableData);
	var selected = $view.list.getSelectRowIds();
	if(selected.length > 1){
		Messenger.post({
			'message': &quot;不能同时处理多个事件&quot;,
            'type': 'error',
        });	
        return;
	}else if(selected.length==0)
	{
		Messenger.post({
			'message': &quot;需要选中一个事件才能进行处理&quot;,
            'type': 'error',
        });	
        return;
	}
	
	var params = &quot;taskId=&quot;+rowData.taskId+&quot;&amp;moduleId=&quot;+rowData.moduleId+&quot;&amp;businessKey=&quot;+rowData.businessKey+&quot;&amp;taskType=todo&quot;

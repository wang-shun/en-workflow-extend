page_onload:function(){
	if(TabOperator!=null){
	/*先保存之前的监听事件*/
		var onSelectBefore = TabOperator.config().events.onSelect;
		if(!TabOperator.config().events.isEventFlowListenAdded){
			TabOperator.config().events.onSelect=function(url,currTab){
				  onSelectBefore(url,currTab);
				  var scope=$('[c2tabset]>.tab-content>.tab-pane.active>div').scope();
				  if(scope){
					  scope.$broadcast('tabActived');
				  }
				  if(url==&quot;f/problemworktodo&quot;||url==&quot;f/eventworktodo&quot;||url==&quot;f/releaseworktodo&quot;||url==&quot;f/changeworktodo&quot;||url==&quot;f/configworkflowtodo&quot;){
					  	/*加入自己的flag，标记自己的监听已经添加完毕*/
					    TabOperator.config().events.isEventFlowListenAdded = true;
			    	  	$params.search = {};
			    	  	$params.search.serviceTypeId = $params.workType;
			    	  	$view.list.refresh($params.search);
			      }
			
			}		
		}	
	}

},


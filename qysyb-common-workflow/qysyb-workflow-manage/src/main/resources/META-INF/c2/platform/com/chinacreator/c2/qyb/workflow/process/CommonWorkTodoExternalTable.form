operateConcernedWork:function(proinsid,moduleId,taskdefid,ConcernStatus,thiz,$event){
	//阻止事件冒泡，点击按钮不需要选中行
	$event.stopPropagation();
	if(ConcernStatus==&quot;undefined&quot;){
		ConcernStatus = undefined;
	}
	console.log(ConcernStatus,ConcernStatus);
	//关注状态为空 则为添加关注工单操作
	if(angular.isUndefined(ConcernStatus)){
		Modal.open(&quot;f/servicemanageinformtypechoose&quot;
			,function(){			
			}
			,function(data){
				if(angular.isUndefined(data.informType)){
					Messenger.post({
						'message': &quot;选择通知方式&quot;,
		                'type': 'error'
		            });					
					return;
				}
				$model.uccentity.processInsId = proinsid;
				/*此处不能用流程定义 因为多个服务产品可能用的是同一个流程定义 此处modelId可以代表一个服务产品*/
				$model.uccentity.processDefId = moduleId;
				$model.uccentity.observerId = $model.curUserId;
				$model.uccentity.catogory = &quot;user&quot;;
				$model.uccentity.informType = data.informType;
				console.log('entity',$model.uccentity);
				//默认邮件通知和短信通知
				$model.uccentity.informType = data.informType;
				$http.post(&quot;ws/operateuserconcernwork&quot;,{ucc:$model.uccentity,userId:$model.curUserId,concernStatus:ConcernStatus,action:kcompglobal.workoperate.concernthiswork})
					.success(function(a){
						$view.list.refresh();
					});
		});
	}else{//关注状态不为空 则为取消关注操作
		$model.uccentity.processInsId = proinsid;
		/*此处不能用流程定义 因为多个服务产品可能用的是同一个流程定义 此处modelId可以代表一个服务产品*/
		$model.uccentity.processDefId = moduleId;
		$model.uccentity.observerId = $model.curUserId;
		$model.uccentity.catogory = &quot;user&quot;;
		/*$model.uccentity.informType = data.informType;*/
		$http.post(&quot;ws/operateuserconcernwork&quot;,{ucc:$model.uccentity,userId:$model.curUserId,concernStatus:ConcernStatus,action:kcompglobal.workoperate.concernthiswork})
			.success(function(a){
				$view.list.refresh();
			});		
	}
		
/*	console.log(proinsid);
	console.log(ConcernStatus);
	console.log(&quot;this&quot;,thiz);*/
	

},

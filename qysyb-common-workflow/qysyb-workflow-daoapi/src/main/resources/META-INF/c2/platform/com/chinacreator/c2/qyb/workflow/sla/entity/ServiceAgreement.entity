        <form:scripts>btn_add_click:function($event){
	if(!angular.isUndefined($model.lastsel)){
		jQuery('#kpilist1').jqGrid('saveRow',$model.lastsel,{
			&quot;url&quot;:&quot;clientArray&quot;});
	}
	var formCheck=$view.mainForm.check();
	if(formCheck==false){
		Messenger.post({type:'error',message:'必填项不能为空'});
		return;
	}	
	$model.entity.$save(function(){
		console.log($model.list);
		var skrs = $view.kpilist1.getRowData();
		$http.get(&quot;ws/editslakpirel&quot;,{&quot;params&quot;:{
			&quot;skrs&quot;:skrs
		}}).success(function(){
			Messenger.post({type:'error',message:'保存成功'});
			Modal.instance.close();	
		});
	});
},

btn_edit_click:function($event){
   Modal.instance.close();
},


page_afterDataSourceLoad:function(){
	$view.kpilist1.setParams({serviceAgreementId:$model.entity.serviceAgreementId},true);
	var slaId = {};
	if(!angular.isUndefined($model.entity.serviceAgreementId)){
		slaId.serviceAgreementId = $model.entity.serviceAgreementId;
		$view.slasplist1.setParams({slaId:slaId});
	}else{
		slaId.serviceAgreementId = &quot;-1&quot;;
		$view.slasplist1.setParams({slaId:slaId},true);
	}
	
},

serviceLevelId1_change:function(){
	console.log($model.entity.serviceLevelId);
},
newField_customFormatter:function(cellvalue,options,rowObject){
	return '&lt;a ng-click=functions.rowedit(\&quot;'+rowObject.relId+'\&quot;)>&lt;img src=&quot;assets/images/serviceproducticon/edit-icon.gif&quot;>&lt;/a>'	
},
rowedit:function(o){
	if(!angular.isUndefined($model.lastsel)&amp;&amp;$model.lastsel!=o){
		jQuery('#kpilist1').jqGrid('saveRow',$model.lastsel,{
			&quot;url&quot;:&quot;clientArray&quot;});
	}
	jQuery('#kpilist1').jqGrid('editRow',o,{
		&quot;url&quot;:&quot;clientArray&quot;});
	$model.lastsel = o;
},

kpilist_onSelectRow:function(rowid,status,e){
	if(!angular.isUndefined($model.lastsel)&amp;&amp;$model.lastsel!=rowid){
		jQuery('#kpilist1').jqGrid('saveRow',$model.lastsel,{
			&quot;url&quot;:&quot;clientArray&quot;});
	}	
},

slaprodiv_onSelect:function(){
	var slaId = {};
	if(!angular.isUndefined($model.entity.serviceAgreementId)){
		slaId.serviceAgreementId = $model.entity.serviceAgreementId;
		$view.slasplist1.setParams({slaId:slaId});
	}else{
		slaId.serviceAgreementId = &quot;-1&quot;;
		$view.slasplist1.setParams({slaId:slaId},true);
	}
},

buttonAdd_click:function(rowId,rowData,tableData){
	Modal.open('ef/slaserviceproductrel/add',{},function(data){
		$http.get(&quot;ws/saveslaproductrel&quot;,{&quot;params&quot;:{&quot;spIds&quot;:data.ids,&quot;slaId&quot;:$model.entity.serviceAgreementId}})
			.success(function(){
						var slaId = {};
						slaId.serviceAgreementId = $model.entity.serviceAgreementId;
						$view.slasplist1.setParams({slaId:slaId});
			});
	});	
},

serviceTypeId1_customFormatter:function(cellvalue,options,rowObject){
	return eval('$model.getDictDatasByType.result.服务分类.'+cellvalue);
},

wfProcessdefid1_customFormatter:function(cellvalue,options,rowObject){
	var a = $model.allProcessDef.result;
	var r;
	a.forEach(function(e){
			if(e.key == cellvalue){
						console.log(e);
						r =  e.name;
						return;
			}
		});
	return r;
},

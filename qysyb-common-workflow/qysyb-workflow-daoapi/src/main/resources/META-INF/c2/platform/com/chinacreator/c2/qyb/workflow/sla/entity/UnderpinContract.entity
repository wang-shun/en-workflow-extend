		return;
	}	
	if(angular.isUndefined($model.entity.ucId)){
	    $model.entity.$save(function(){
	    	$view.ucaddserviceproducttab.setDisabledState(false);
        	Messenger.post({type:'success',message:'保存成功'});
	    });
	}else{
		Modal.instance.close();
	}
},

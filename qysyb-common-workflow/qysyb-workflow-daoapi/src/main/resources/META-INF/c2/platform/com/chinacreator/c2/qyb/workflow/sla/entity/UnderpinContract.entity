   Modal.instance.close();
},

//选择合同
contractName_customEven:function(){
	Modal.open(&quot;f/contractChooseForm&quot;,function(a){}
		,function(a){
			 var list = a.result;
			 if(list.length>1){
				 Messenger.post({type:'error',message:'请只选择一个合同'});
				 return;
			 }else if(list.length&lt;=0){
				 Messenger.post({type:'error',message:'请选择合同'});
			 }else{
				 $model.entity.contractName = a.result[0].contractName;
			 }
			
		})
},

buttonAdd_click:function(rowId,rowData,tableData){
	Modal.open('ef/slaserviceproductrel/add',{},function(data){
		$http.post(&quot;ws/saveserviceproductucrel&quot;,{ucId:$model.entity.ucId,spIds:data.ids})
		.success(function(){
			$view.list.refresh();			
		});
	});	
	
},

ucaddserviceproducttab_onSelect:function(){
	var ucId = {};
	ucId.ucId = $model.entity.ucId;
	$view.list.setParams({ucId:ucId});
},

ucId_customFormatter:function(cellvalue,options,rowObject){
	return &quot;{{$model.dictdatalist.result['服务分类']['&quot;+cellvalue+&quot;']}}&quot;;

  <form:scripts>

page_afterDataSourceLoad:function(){

	$model.todoworktotalbyst.$load({'userId':$model.curuser.result.id});
	$model.a = $model.allservicetype.result[&quot;服务分类&quot;];
	$view.list1.setParams({&quot;serviceTypeId&quot;:$model.a[0].dictdataName})
},

prio_customFormatter:function(cellvalue,options,rowObject){
	if(!angular.isUndefined($model.priority.result)){
		var ary = $model.priority.result['优先级'];
		for(var i=0;i&lt;ary.length;i++){
			if(ary[i].dictdataName==cellvalue){
				return ary[i].dictdataValue;
			}
		}
		return &quot;无&quot;;
	}	
},
refreshtodolist:function(a){
	$view.list1.setParams({&quot;serviceTypeId&quot;:a})
},

newField_customFormatter:function(cellvalue,options,rowObject){
	var a;
	var b;
	if(rowObject.isHandleTimeout == true){
		b = '&lt;img class=&quot;rdtalarm&quot; title=&quot;解决时间告警&quot; data-rdt=&quot;117&quot; data-dst=&quot;120&quot; data-dot=&quot;0&quot; src=&quot;assets/images/OTime_red.gif&quot;>'
	}else if((rowObject.isHandleTimeout == false)){
		b = '&lt;img class=&quot;rdtalarm&quot; title=&quot;解决时间告警&quot; data-rdt=&quot;117&quot; data-dst=&quot;120&quot; data-dot=&quot;0&quot; src=&quot;assets/images/noOTime_green.gif&quot;>'
	}else{
		b = '&lt;img class=&quot;rdtalarm&quot; title=&quot;无SLA数据&quot; data-rdt=&quot;117&quot; data-dst=&quot;120&quot; data-dot=&quot;0&quot; src=&quot;assets/images/stop.gif&quot;>'
	}
	if(rowObject.isRecieveTimeout == true){
		a = '&lt;img class=&quot;&quot; title=&quot;响应时间告警 &quot; data-rrt=&quot;0&quot; data-rst=&quot;60&quot; data-rot=&quot;0&quot; src=&quot;assets/images/OTime_red.gif&quot;>'
	}else if((rowObject.isRecieveTimeout == false)){
		a = '&lt;img class=&quot;&quot; title=&quot;响应时间告警 &quot; data-rrt=&quot;0&quot; data-rst=&quot;60&quot; data-rot=&quot;0&quot; src=&quot;assets/images/noOTime_green.gif&quot;>'
	}else{
		a = '&lt;img class=&quot;rdtalarm&quot; title=&quot;无SLA数据&quot; data-rdt=&quot;117&quot; data-dst=&quot;120&quot; data-dot=&quot;0&quot; src=&quot;assets/images/stop.gif&quot;>'
	}
	return a+b;	
},

newField5_customFormatter:function(cellvalue,options,rowObject){
	var a;
	if(!angular.isUndefined(rowObject.slaRemaintime)){
		return '&lt;span class=&quot;sla_remainder&quot; style=&quot;color:red&quot;>'+rowObject.slaRemaintime+'&lt;/span>分钟'
	}else{
		return '&amp;nbsp';
	}	
},

newField12_customFormatter:function(cellvalue,options,rowObject){
	return $model.allservicetps.result[&quot;服务分类&quot;][cellvalue];
},

title1_customFormatter:function(cellvalue,options,rowObject){
	return '&lt;a href=&quot;javascript:void(0)&quot; ng-click=functions.workclick(&quot;'+rowObject.taskId+'&quot;,&quot;'+rowObject.moduleId+'&quot;,&quot;'+rowObject.businessKey+'&quot;)>'+cellvalue+'&lt;/a>'	
},
workclick:function(a,b,c){
	var params = &quot;taskId=&quot;+a+&quot;&amp;moduleId=&quot;+b+&quot;&amp;businessKey=&quot;+c+&quot;&amp;taskType=todo&quot;
	location.href = &quot;#/wf/taskHandle?&quot;+params;
}</form:scripts>

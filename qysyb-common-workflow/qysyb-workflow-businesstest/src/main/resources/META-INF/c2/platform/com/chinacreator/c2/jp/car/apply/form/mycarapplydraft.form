loadusers:function(){
//  didnt work  TODO
	if($scope.users==undefined){
		$http.post(&quot;ws/getAllUserinfo&quot;,{})
			.success(function(data){
				$timeout(function(){
					$scope.$apply(function(){
						$scope.users = data.result;
					});
					console.log(&quot;users&quot;,$scope.users);
				});

			});
	}
},


    <form:datasource xsi:type="form:PagingDataSource" id="list" name="列表型数据源" elementType="entity:com.chinacreator.c2.jp.car.manage.entity.car" logic="com.chinacreator.c2.jp.car.provider.CarListContentProvider" pageSize="10" type="custom"/>
  </form:datasources>
  <form:scripts>

buttonAdd_click:function(rowId,rowData,tableData){
	Modal.open(&quot;ef/car_manage_entity/add&quot;,{subject:$scope.subject},
			function(){
				$view.list.refresh();
			}
	);
},

newField_click:function(rowId,rowData,tableData){
	var selected = $view.list.getSelectDatas();
	var params = {cars:selected};
	location.href = '#/f/carcalendar?cars='+JSON.stringify(params.cars);
//	Modal.open(&quot;f/carcalendar&quot;,{cars:selected});
},

newField6_click:function($event){
	$view.mainForm.clean();	
},

newField5_click:function($event){
	$view.list.refresh($params.search);
},

buttonDel_click:function(rowId,rowData,tableData){
	var selectNodes = $view.list.getSelectDatas();
	if(angular.isArray(selectNodes) &amp;&amp; selectNodes.length>0){
		var ids = new Array();
		for(var a =0;a&lt;selectNodes.length;a++){
			ids.push(selectNodes[a].oid);
		}
		Modal.openDelConfirm({'message':&quot;确定删除选中的车辆吗？&quot;},
         function(){
			$http.post(&quot;ws/deletecar&quot;,{ids:ids})
			.success(function(data){
				Messenger.post({
					'message': &quot;删除成功！&quot;,
					'type': 'success',
				});
				$view.list.refresh();
			});	
         },
        function(){});
	}else{
		Messenger.post({
		    'message': &quot;请选择需要删除的信息！&quot;,
		    'type': 'error',
		});
	}		
}</form:scripts>
